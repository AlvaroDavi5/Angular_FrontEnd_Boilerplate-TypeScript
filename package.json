{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "angular_frontend_boilerplate",
	"version": "1.0.0",
	"description": "Node.js Boilerplate for Back-End using TypeScript",
	"license": "MIT",
	"private": false,
	"author": "Álvaro Davi Santos Alves <alvaro-davi1@hotmail.com>",
	"contributors": [
		"Álvaro Davi Santos Alves <alvaro-davi1@hotmail.com>"
	],
	"repository": "https://github.com/AlvaroDavi5/Angular_FrontEnd_Boilerplate-TypeScript",
	"main": "./src/main.ts",
	"engineStrict": true,
	"engines": {
		"node": ">=22.0.x <=24.x.x",
		"npm": ">=10.9.x <=11.x.x",
		"yarn": ">=1.22.x <2.x.x"
	},
	"os": [
		"linux",
		"ubuntu",
		"win32"
	],
	"keywords": [
		"boilerplate",
		"node",
		"typescript",
		"javascript",
		"angular"
	],
	"scripts": {
		"typecheck": "tsc --project tsconfig.json --noEmit",
		"build": "ng build",
		"build:watch": "ng build --watch --configuration development",
		"start": "ng serve --port 3001",
		"start:dev": "ng serve --port 3001 --open",
		"serve:ssr:angular_frontend_boilerplate": "node dist/angular_frontend_boilerplate/server/server.mjs",
		"security-check": "node ./scripts/security-check.js",
		"mock-dependencies": "cross-env NODE_PATH=. ts-node --project tsconfig.script.json ./scripts/mock-dependencies.ts",
		"test": "ng test --watch=true --browsers=ChromeHeadless",
		"prettify": "",
		"commit": "",
		"postinstall": ""
	},
	"config": {},
	"dependencies": {
		"@angular/animations": "^20.3.4",
		"@angular/common": "^20.2.0",
		"@angular/compiler": "^20.2.0",
		"@angular/core": "^20.2.0",
		"@angular/forms": "^20.2.0",
		"@angular/platform-browser": "^20.2.0",
		"@angular/platform-browser-dynamic": "^20.3.4",
		"@angular/platform-server": "^20.2.0",
		"@angular/router": "^20.2.0",
		"@angular/ssr": "^20.3.6",
		"express": "^5.1.0",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.15.0"
	},
	"devDependencies": {
		"@angular/build": "^20.2.2",
		"@angular/cli": "^20.2.2",
		"@angular/compiler-cli": "^20.2.0",
		"@types/express": "^5.0.1",
		"@types/jasmine": "~5.1.0",
		"@types/node": "^20.17.19",
		"child_process": "^1.0.2",
		"cross-env": "^10.1.0",
		"jasmine-core": "~5.9.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "~5.9.2"
	}
}